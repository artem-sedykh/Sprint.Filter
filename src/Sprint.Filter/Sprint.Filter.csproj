<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net46</TargetFramework>
    <RootNamespace>Sprint</RootNamespace>
    <Description>Filtering data in ASP.NET MVC</Description>
    <PackageTags>ASP.NET MVC, Filter, Sprint</PackageTags>
    <PackageProjectUrl>https://github.com/artem-sedykh/Sprint.Filter</PackageProjectUrl>
    <RepositoryUrl>https://github.com/artem-sedykh/Sprint.Filter</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Authors>Artem Sedykh</Authors>
    <Company>Sprint.Filter</Company>
    <Version>3.2.4</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNet.Mvc" Version="4.0.20505" />
    <PackageReference Include="Sprint.Linq" Version="1.2.10" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="System.Web.Extensions">
      <HintPath>..\..\..\..\..\Program Files (x86)\Reference Assemblies\Microsoft\Framework\.NETFramework\v4.0\System.Web.Extensions.dll</HintPath>
    </Reference>
  </ItemGroup>

